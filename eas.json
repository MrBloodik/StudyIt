{
  "cli": {
    "version": ">= 15.0.10",
    "appVersionSource": "remote"
  },
  "build": {
    "ios": {
      "node": "24.7.0",
      "yarn": "1.22.22",
      "resourceClass": "medium"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "developmentClient": false,
      "node": "24.7.0",
      "yarn": "1.22.22",
      "env": {
        "EXPO_NO_DEV_CLIENT": "1"
      },
      "android": {
        "resourceClass": "medium"
      },
      "ios": {
        "resourceClass": "medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
